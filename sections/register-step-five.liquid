<div class="hidden" id="RegisterStepFive">
  {%- form 'customer_login', novalidate: 'novalidate' -%}
    <input
      :value="customerData.email"
      class="hidden"
      name="customer[email]"
      type="email">
    <input
      :value="customerData.password"
      class="hidden"
      name="customer[password]"
      type="password">
    <p class="success-text" v-if="redirectCustomer">Pendaftaran berhasil.</p>
    <p class="fail-text" v-else>Pendaftaran gagal.</p>
    <button @click="loading = true" type="submit">
      <span v-if="!loading">Lanjut untuk belanja</span>
      <span id="load-animate" v-if="loading">Tunggu sebentar</span>
    </button>
  {%- endform -%}
</div>